@article{bell2015Misdiagnosis,
  title = {Misdiagnosis of {{African-Americans}} with {{Psychiatric Issues}} - {{Part II}}},
  author = {Bell, Carl C. and Jackson, Willie Mae and Bell, Briatta H.},
