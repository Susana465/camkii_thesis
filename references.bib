@article{cheriyan2011Calcium,
  title = {Calcium/{{Calmodulin Dependent Protein Kinase II Bound}} to {{NMDA Receptor 2B Subunit Exhibits Increased ATP Affinity}} and {{Attenuated Dephosphorylation}}},
