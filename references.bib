@article{li2020High,
  title = {High Neural Activity Accelerates the Decline of Cognitive Plasticity with Age in {{Caenorhabditis}} Elegans},
  author = {Li, Qiaochu and Marcu, Daniel-Cosmin and Palazzo, Ottavia and Turner, Frances and King, Declan and Spires-Jones, Tara L and Stefan, Melanie I and Busch, Karl Emanuel},
  editor = {Sengupta, Piali and Portman, Douglas and Hoppe, Thorsten},
  date = {2020-11-24},
  journaltitle = {eLife},
  volume = {9},
  pages = {e59711},
  publisher = {eLife Sciences Publications, Ltd},
  issn = {2050-084X},
  doi = {10.7554/eLife.59711},
  url = {https://doi.org/10.7554/eLife.59711},
  urldate = {2024-01-19},
  abstract = {The ability to learn progressively declines with age. Neural hyperactivity has been implicated in impairing cognitive plasticity with age, but the molecular mechanisms remain elusive. Here, we show that chronic excitation of the Caenorhabditis elegans O2-sensing neurons during ageing causes a rapid decline of experience-dependent plasticity in response to environmental O2 concentration, whereas sustaining lower activity of O2-sensing neurons retains plasticity with age. We demonstrate that neural activity alters the ageing trajectory in the transcriptome of O2-sensing neurons, and our data suggest that high-activity neurons redirect resources from maintaining plasticity to sustaining continuous firing. Sustaining plasticity with age requires the K+-dependent Na+/Ca2+ (NCKX) exchanger, whereas the decline of plasticity with age in high-activity neurons acts through calmodulin and the scaffold protein Kidins220. Our findings demonstrate directly that the activity of neurons alters neuronal homeostasis to govern the age-related decline of neural plasticity and throw light on the mechanisms involved.},
  keywords = {Caenorhabditis elegans,experience-dependent plasticity,learning and memory,neural ageing,neuronal Ca2+ homeostasis,oxygen-sensing neurons,plasticity decline,transcriptomics},
  file = {C:\Users\susan\Zotero\storage\58C9IGXS\Li et al_2020_High neural activity accelerates the decline of cognitive plasticity with age.pdf}
}

