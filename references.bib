@article{fujii2003Cooperativity,
  title = {Cooperativity between Activation of Metabotropic Glutamate Receptors and {{NMDA}} Receptors in the Induction of {{LTP}} in Hippocampal {{CA1}} Neurons},
  author = {Fujii, Satoshi and Mikoshiba, Katsuhiko and Kuroda, Yoichiro and Ahmed, Taufiq M. and Kato, Hiroshi},
  date = {2003-08-01},
  journaltitle = {Neuroscience Research},
  volume = {46},
  number = {4},
  pages = {509--521},
  issn = {0168-0102},
  doi = {10.1016/S0168-0102(03)00162-7},
  url = {https://www.sciencedirect.com/science/article/pii/S0168010203001627},
  urldate = {2025-01-17},
  abstract = {In CA1 neurons of guinea pig hippocampal slices, long-term potentiation (LTP) was induced by 10 min application of 10 μM aminocyclopentane-1S, 3R-dicarboxylic acid (ACPD), the metabotropic glutamate receptor (mGluR) agonist, in the presence of test synaptic inputs (once every 20 s). In contrast, long-term depression (LTD) was induced by application of 10 μM ACPD in the absence of test inputs. When 10 μM ACPD was applied in the presence of test inputs, co-application of the N-methyl-d-aspartate (NMDA) receptor antagonist, d,l-2-amino-5-phosphonovalerate resulted in LTD induction when used at 50 μM. In ACPD-induced LTP, the delivery of test synaptic inputs to CA1 neurons could be replaced by co-application of NMDA (100 nM) during ACPD perfusion. These results suggest that, in CA1 neurons, a co-operative effect involving the activation of both mGluRs and NMDA receptors is required to trigger the process involved in ACPD-induced LTP. In addition, ACPD-induced LTD was blocked by co-application of an inositol 1,4,5-trisphosphate (IP3) receptor inhibitor, 2-aminotheoxydiphenyl borate (10 μM), which had no effect on ACPD-induced LTP. The results of the present study, therefore, indicate that ACPD-induced LTP involves NMDA receptors, but not IP3 receptors, whereas the converse applies to ACPD-induced LTD.},
  keywords = {-methyl--aspartate,Hippocampus,Inositol trisphosphate receptors,Long-term depression,Long-term potentiation,Metabotropic glutamate receptors},
  file = {C:\Users\susan\Zotero\storage\W2YTWN29\S0168010203001627.html}
}

@article{fukunaga1996CaM,
  title = {{{CaM KINASE II IN LONG-TERM POTENTIATION}}},
  author = {family=Fukunaga, given=KOHJI, given-i=KOHJI and family=Muller, given=DOMINIQUE, given-i=DOMINIQUE and family=Miyamoto, given=EISHICHI, given-i=EISHICHI},
  date = {1996-04-01},
  journaltitle = {Neurochemistry International},
  volume = {28},
