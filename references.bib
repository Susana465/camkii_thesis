@article{palmer2004MultipleDevelopmentallyRegulated,
  title = {Multiple, {{Developmentally Regulated Expression Mechanisms}} of {{Long-Term Potentiation}} at {{CA1 Synapses}}},
  author = {Palmer, Mary J. and Isaac, John T. R. and Collingridge, Graham L.},
  date = {2004-05-26},
  journaltitle = {J. Neurosci.},
  volume = {24},
