@article{schaff2016Rulebased,
  title = {Rule-Based Modeling with {{Virtual Cell}}},
  author = {Schaff, James C. and Vasilescu, Dan and Moraru, Ion I. and Loew, Leslie M. and Blinov, Michael L.},
  date = {2016-09-15},
  journaltitle = {Bioinformatics},
