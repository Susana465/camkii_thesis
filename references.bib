@article{chen1996Longterm,
  title = {Long-Term Modifications of Synaptic Efficacy in the Human Inferior and Middle Temporal Cortex.},
  author = {Chen, W R and Lee, S and Kato, K and Spencer, D D and Shepherd, G M and Williamson, A},
  date = {1996-07-23},
  journaltitle = {Proceedings of the National Academy of Sciences},
  volume = {93},
