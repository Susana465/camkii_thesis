@article{byrne2010Cellular,
  title = {Cellular {{Dynamic Simulator}}: {{An Event Driven Molecular Simulation Environment}} for {{Cellular Physiology}}},
  shorttitle = {Cellular {{Dynamic Simulator}}},
  author = {Byrne, Michael J. and Waxham, M. Neal and Kubota, Yoshihisa},
  date = {2010-06-01},
