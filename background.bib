  urldate = {2025-03-29},
  abstract = {The fast excitatory synaptic transmission in the mammalian central nervous system (CNS) is mediated by L-glutamate, which activates cation-selective receptor channels located in postsynaptic membranes. Physiological and biophysical studies indicate the existence of different glutamate receptor channels with distinct gating kinetics, ion permeabilities, and conductances. The main channel classes based on distinct pharmacological properties are termed N-methyl-D-aspartate (NMDA) and non-NMDA receptors. The CNS expression characteristics of NR2 subunits described in this chapter suggest a complex genetic control determining which central neurons carry which NMDA receptor subtypes. A study of recombinantly expressed NMDA receptors described in this chapter indicated significant differences in the properties of four NR1-NR2 receptors. An important function of NMDA channels is to detect synchronicity in pre- and postsynaptic depolarization events. The changes in the kinetic properties of NMDA receptor channels are present in layer IV neurons of the developing rat visual cortex. In these neurons, NMDA-receptor mediated postsynaptic currents decay with double exponential time courses. During development, the contribution of the slower component progressively decreased. The slow component persisted in dark-reared animals, indicating long-term modifications of native NMDA-receptor gating properties, which depend on the electrical activity of these neurons. The progressive increase in NR2A expression with a concomitant decline in NR2C or NR2D expression in cortical layers might underlie the observed modification in NMDA channel gating.},
  isbn = {978-0-12-571150-0},
  file = {C:\Users\susan\Zotero\storage\2QJRQAT6\B9780125711500500068.html}
}

@online{sever2019BioRxiv,
  title = {{{bioRxiv}}: The Preprint Server for Biology},
  shorttitle = {{{bioRxiv}}},
  author = {Sever, Richard and Roeder, Ted and Hindle, Samantha and Sussman, Linda and Black, Kevin-John and Argentine, Janet and Manos, Wayne and Inglis, John R.},
