@article{kampa2004Kinetics,
  title = {Kinetics of {{Mg2}}+ Unblock of {{NMDA}} Receptors: Implications for Spike-Timing Dependent Synaptic Plasticity},
  shorttitle = {Kinetics of {{Mg2}}+ Unblock of {{NMDA}} Receptors},
  author = {Kampa, Björn M. and Clements, John and Jonas, Peter and Stuart, Greg J.},
  date = {2004},
  journaltitle = {The Journal of Physiology},
  volume = {556},
  number = {2},
