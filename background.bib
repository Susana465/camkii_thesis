@article{buard2010CaMKII,
  title = {{{CaMKII}} “{{Autonomy}}” {{Is Required}} for {{Initiating But Not}} for {{Maintaining Neuronal Long-Term Information Storage}}},
  author = {Buard, Isabelle and Coultrap, Steven J. and Freund, Ronald K. and Lee, Yong-Seok and Dell'Acqua, Mark L. and Silva, Alcino J. and Bayer, K. Ulrich},
