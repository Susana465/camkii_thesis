  author = {Johnson, M. E. and Chen, A. and Faeder, J. R. and Henning, P. and Moraru, I. I. and Meier-Schellersheim, M. and Murphy, R. F. and Prüstel, T. and Theriot, J. A. and Uhrmacher, A. M.},
