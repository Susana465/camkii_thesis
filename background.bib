@article{hartgerink2017Too,
  title = {Too {{Good}} to Be {{False}}: {{Nonsignificant Results Revisited}}},
  shorttitle = {Too {{Good}} to Be {{False}}},
  author = {Hartgerink, C. H. J. and Wicherts, J. M. and family=Assen, given=M. A. L. M., prefix=van, useprefix=true},
