@article{grover1998Evidence,
  title = {Evidence for Postsynaptic Induction and Expression of {{NMDA}} Receptor Independent {{LTP}}},
  author = {Grover, L. M.},
  date = {1998-03},
  journaltitle = {J Neurophysiol},
  volume = {79},
  number = {3},
  eprint = {9497399},
