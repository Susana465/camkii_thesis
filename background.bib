  booktitle = {Progress in {{Brain Research}}},
  author = {Sacktor, Todd Charlton},
  editor = {Sossin, Wayne S. and Lacaille, Jean-Claude and Castellucci, Vincent F. and Belleville, Sylvie},
  date = {2008-01-01},
