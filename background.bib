@article{coultrap2012CaMKII,
  title = {{{CaMKII}} Regulation in Information Processing and Storage},
  author = {Coultrap, Steven J. and Bayer, K. Ulrich},
  date = {2012-10-01},
  journaltitle = {Trends in Neurosciences},
  volume = {35},
