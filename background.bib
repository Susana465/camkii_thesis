@article{ruppersberg1994Mechanism,
  title = {The Mechanism of Magnesium Block of {{NMDA}} Receptors},
  author = {Ruppersberg, J. Peter and family=Kitzing, given=Eberhard, prefix=v., useprefix=false and Schoepfer, Ralf},
