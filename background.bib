@article{halt2012CaMKIIb,
  title = {{{CaMKII}} Binding to {{GluN2B}} Is Critical during Memory Consolidation},
  author = {Halt, Amy R and Dallapiazza, Robert F and Zhou, Yu and Stein, Ivar S and Qian, Hai and Juntti, Scott and Wojcik, Sonja and Brose, Nils and Silva, Alcino J and Hell, Johannes W},
  date = {2012-03-07},
