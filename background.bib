@article{dupont2003Sensitivity,
  title = {Sensitivity of {{CaM}} Kinase {{II}} to the Frequency of {{Ca2}}+ Oscillations: A Simple Model},
  shorttitle = {Sensitivity of {{CaM}} Kinase {{II}} to the Frequency of {{Ca2}}+ Oscillations},
