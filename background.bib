  author = {family=Fukunaga, given=KOHJI, given-i=KOHJI and family=Muller, given=DOMINIQUE, given-i=DOMINIQUE and family=Miyamoto, given=EISHICHI, given-i=EISHICHI},
  date = {1996-04-01},
  journaltitle = {Neurochemistry International},
