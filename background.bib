@article{yang1999Structural,
  title = {Structural {{Examination}} of {{Autoregulation}} of {{Multifunctional Calcium}}/{{Calmodulin-dependent Protein Kinase II}}*},
  author = {Yang, Eungyeong and Schulman, Howard},
  date = {1999-09-10},
  journaltitle = {Journal of Biological Chemistry},
  volume = {274},
